{
  "expo": {
    "name": "react-native-chat",
    "slug": "react-native-chat",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.getstream.react-native-chat-chucky"
    },
    "android": {
      "icon": "./assets/android.png",
      "package": "io.getstream.chucky",
      "versionCode": 1
    }
  }
}
